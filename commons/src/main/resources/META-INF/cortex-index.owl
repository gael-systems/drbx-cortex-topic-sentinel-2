         <drb:parentClassTest>&s2;datastrip</drb:parentClassTest>
         <drb:nameMatch>(S2[AB]_...._MTD_L(0_|1A|1B|1C|2A)_DS_.*|MTD_DS\.xml)</drb:nameMatch>
      </drb:signature>
   </owl:Class>

